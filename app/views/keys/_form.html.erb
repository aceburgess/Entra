<%= form_for @key do |f| %>
<%= f.label :place_id, "Select Place" %>
<%= f.collection_select(:place_id, places, :id, :nickname)%></br>
<%= f.label :guest_id, "Select Guest" %>
<%= f.collection_select(:guest_id, guests, :id, :name)%></br>
<%= f.label :start_date, "Select Start Time" %>
<%= f.datetime_select :start_date, :ampm => true, :order => [:month, :day, :year, :hour], :date_separator => "", :discard_minute => true%></br>
<%= f.label :end_date, "Select End Time" %>
<%= f.datetime_select :end_date, :ampm => true, :order => [:month, :day, :year, :hour], :date_separator => "", :discard_minute => true%></br>

<%=  f.submit %>
<% end %>